<odoo>
    <data>
        <record id="view_stock_picking_form_view_ups" model="ir.ui.view">
            <field name="name">stock.picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
             <field name="arch" type="xml">
             <xpath expr="//header" position="inside">
                 <button name="create_ups_label" string="UPS label" type="object"
                         attrs="{'invisible': ['|','|',('picking_type_code', '=', 'outgoing'),('state','in',('done','cancel')),('carrier_tracking_ref','!=',False)]}"/>
                 <button name="export_order_to_ups" string="Export to UPS" type="object"
                         attrs="{'invisible': ['|','|','|','|',('picking_type_code', '!=', 'outgoing'),('state','!=','done'),('carrier_tracking_ref','!=',False),('carrier_id','=',False),('delivery_type','!=','ups_shipping_provider')]}"/>
             </xpath>
              <xpath expr="//field[@name='carrier_id']" position="after">
                    <field name="ups_cod_bulk_value" readonly="False" attrs="{'invisible': [('delivery_type', '!=', 'ups_shipping_provider')]}"/>
                    <field name="insured_amount" attrs="{'invisible': [('delivery_type', '!=', 'ups_shipping_provider')]}" />
             </xpath>
            </field>
        </record>
    </data>
</odoo>