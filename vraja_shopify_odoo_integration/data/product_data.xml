<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Create a shopify discount product-->
        <record id="discount_product" model="product.product">
            <field name="name">Shopify Discount Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">shopify_discount_product</field>
        </record>

        <!--Create a shopify shipping product-->
        <record id="shipping_product" model="product.product">
            <field name="name">Shopify Shipping Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">shopify_shipping_product</field>
        </record>

        <!--Create a shopify gift card product-->
        <record id="gift_card_product" model="product.product">
            <field name="name">Shopify Gift Card Product</field>
            <field name="detailed_type">service</field>
            <field name="default_code">shopify_gift_card_product</field>
        </record>
    </data>
</odoo>
