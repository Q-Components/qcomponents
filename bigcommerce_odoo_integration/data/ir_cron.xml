<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="bigcommerce_stock_adjustment_cronjob" model="ir.cron">
            <field name="name">Stock Adjustment</field>
            <field name="model_id" ref="model_stock_inventory"/>
            <field name="state">code</field>
            <field name="code">model.bigcommerce_to_odoo_import_inventory_using_cronjob()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record forcecreate="True" id="bigcommerce_auto_update_pages_cron_job" model="ir.cron">
            <field name="name">Auto Update Source and Destination Page Cron</field>
            <field name="model_id" ref="model_bigcommerce_store_configuration"/>
            <field name="state">code</field>
            <field name="code">model.auto_update_pages_for_import_product()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">25</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>